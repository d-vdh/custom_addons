{
  "name": "Let's Encrypt acme.sh",
  "version": "0.0.8",
  "slug": "letsencrypt_acme",
  "description": "Manage certificate from Let's Encrypt",
  "url": "https://github.com/roterBulle/custom_addons/tree/main/letsencrypt_acme",
  "arch": ["armv7"],
  "startup": "once",
  "init": false,
  "boot": "manual",
  "map": ["ssl:rw", "share"],
  "options": {
    "email": null,
    "domains": [null],
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "ionos_prefix": null,
    "ionos_secret": null
  },
  "schema": {
    "email": "email",
    "domains": ["str"],
    "certfile": "str",
    "keyfile": "str",
    "ionos_prefix": "str",
    "ionos_secret": "str"
  }
}
